
rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Allow any authenticated user to read and write to any document.
    // WARNING: This is for debugging purposes ONLY. These rules should be
    // secured before launching to production.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
